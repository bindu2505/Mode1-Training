package demo;

class First{
	public void show(){
		System.out.println("base class");
	}
}

class Second extends First{
	public void show(){
		super.show();
		System.out.println("derived class");
	}
}

public class InhDemo {
	
	public static void main(String[] args) {
		Second obj=new Second();
		obj.show();
	}

}
