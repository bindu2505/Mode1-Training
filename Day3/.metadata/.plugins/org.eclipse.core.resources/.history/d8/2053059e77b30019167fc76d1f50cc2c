package com.hcl.ex;

class Cl1{
	int a,b;
	public C1(){
		a=5;b=8;
	}
	@Override
	public String toString() {
		return "C1 [a=" + a + ", b=" + b + "]";
	}
	
}

class Cl2 extends Cl1{
	public C2(int a,int b){
		
	}
}

public class Quiz2 {
	public static void main(String[] args) {
		Cl2 obj=new Cl2(12,12);
		System.out.println(obj);
	}

}
