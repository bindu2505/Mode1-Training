package com.hcl.java;

public class StrFunctionDemo {
	
	public static void main(String[] args) {
		String str="welcome to java programmimg";
		System.out.println("length  "+ str.length());
		System.out.println("char at 5th position  "+ str.charAt(5));
		System.out.println("index of 'a' is "+ str.indexOf('a'));
		System.out.println("last occurance of 'a'is "+ str.lastIndexOf('a'));
		System.out.println("substring "+ str.substring(3,8));
		
		String s1="hello",s2="hello";
		System.out.println(s1.equals(s2));
		
		
	}

}
