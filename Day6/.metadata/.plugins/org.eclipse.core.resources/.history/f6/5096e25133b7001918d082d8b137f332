package com.hcl.collections;

class Data{
	public void swap(int x , int y){
		int t;
		t = x;
		x = y;
		y = t;
		System.out.println("x value " + x + " y value " + y);
		
	}
}

public class SwapDemo {
	public static void main(String[] args) {
		Data obj = new Data();
		int x = 12;
		int y = 13;
		obj.swap(x, y);
		
	}

}
