package com.hcl.collections;

import java.util.HashMap;
import java.util.Map;

public class MapDemo {
	public static void main(String[] args) {
		Map m= new HashMap();
		m.put("priya", "priya");
		m.put("laksh", "laksh");
		m.put("rishab", "hcl");
		m.put("bindu", "shree");
		
		String key = "laksh";
		String value = (String)m.getOrDefault(key, "not found");
		System.out.println(value);
		
	}

}
