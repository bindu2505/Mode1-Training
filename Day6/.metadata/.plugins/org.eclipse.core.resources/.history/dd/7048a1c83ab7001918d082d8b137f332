package com.hcl.collections;

import java.util.ArrayList;
import java.util.List;

public class GenStudent {
	public static void main(String[] args) {
		List<Student> lstStudent = new ArrayList<Student>();
		lstStudent.add(new Student("bindu", "shree", "hassan",9.5 ));
		lstStudent.add(new Student("priya", "darshini", "trichy",10 ));
		lstStudent.add(new Student("hema", "j", "chennai",10 ));
		lstStudent.add(new Student("janani", "v", "chennai",10 ));
		lstStudent.add(new Student("jehanat", "jamil", "delhi", 8.5));
	}

}
